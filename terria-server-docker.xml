<?xml version="1.0" encoding="utf-8"?>
<Containers>
    <Date>2019-06-15</Date>
    <Beta>False</Beta>
    <Name>Terraria server - Tmod</Name>
    <Overview>
        Runs a Tmod terraria server
    </Overview>
    <Description>
        Runs a Tmod terraria server
    </Description>
    <Registry>https://registry.hub.docker.com/u/skippythesnake/terraria-tmod-server</Registry>
    <GitHub>https://github.com/SkippyTheSnake/tmod-terraria-server-docker</GitHub>
    <Repository>skippythesnake/terraria-tmod-server</Repository>
    <Support>https://github.com/SkippyTheSnake/tmod-terraria-server-docker</Support>
    <BindTime>true</BindTime>
    <Privileged>false</Privileged>
    <Networking>
        <Mode>host</Mode>
        <Publish/>
    </Networking>
    <Data>
        <Volume>
            <HostDir>/mnt/user/appdata/tmod-terraria-server/data</HostDir>
            <ContainerDir>/server</ContainerDir>
            <Mode>rw</Mode>
        </Volume>
    </Data>
    <Icon>
        http://chittagongit.com/images/terraria-icon-png/terraria-icon-png-8.jpg
    </Icon>
</Containers>